                    <!--<item name="index" xsi:type="string">banner_id</item>-->
                <!--</item>-->
            <!--</argument>-->
        <!--</column>-->
        <!--<column name="banner_url">-->
            <!--<argument name="data" xsi:type="array">-->
                <!--<item name="config" xsi:type="array">-->
                    <!--<item name="filter" xsi:type="string">text</item>-->
                    <!--&lt;!&ndash;<item name="editor" xsi:type="array">&ndash;&gt;-->
                    <!--&lt;!&ndash;<item name="editorType" xsi:type="string">text</item>&ndash;&gt;-->
